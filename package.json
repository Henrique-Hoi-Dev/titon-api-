{
    "name": "api-titon",
    "version": "2.0.0",
    "description": "API para gerenciamento de titon",
    "main": "server.js",
    "type": "module",
    "imports": {
        "#/*": "./*"
    },
    "scripts": {
        "test": "npm run test:unit && npm run test:integration",
        "test:unit": "cross-env NODE_ENV=test node --experimental-vm-modules ./node_modules/jest/bin/jest.js --runInBand --detectOpenHandles --verbose",
        "test:integration": "cross-env NODE_ENV=test node --experimental-vm-modules ./node_modules/jest/bin/jest.js --projects ./tests --runInBand --detectOpenHandles --verbose",
        "test:integration-silent": "cross-env NODE_ENV=test jest --projects ./tests --runInBand --silent",
        "test:migrate": "cross-env NODE_ENV=test npx sequelize-cli db:migrate",
        "test:seed": "cross-env NODE_ENV=test npx sequelize-cli db:seed:all",
        "single-test": "cross-env NODE_ENV=test jest -t",
        "coverage": "npm run test -- --coverage --silent=false",
        "dev": "cross-env NODE_ENV=development nodemon server.js",
        "dev:debug": "cross-env NODE_ENV=development node server.js",
        "dev:migrate": "cross-env NODE_ENV=development npx sequelize-cli db:migrate",
        "dev:seed": "cross-env NODE_ENV=development npx sequelize-cli db:seed:all",
        "start": "node server.js",
        "start:migrate": "npx sequelize-cli db:migrate",
        "start:seed": "npx sequelize-cli db:seed:all",
        "lint": "eslint . --ext .js",
        "lint:fix": "eslint . --ext .js --fix",
        "style:check": "prettier --check \"**/*.js\"",
        "style:fix": "prettier --write \"**/*.js\"",
        "prepare": "npx husky install"
    },
    "author": "Henrique Hoinacki",
    "license": "MIT",
    "engines": {
        "node": ">=18.20.0 <20 || >=20.12.1"
    },
    "dependencies": {
        "aws-sdk": "^2.1692.0",
        "axios": "^1.9.0",
        "bcrypt": "^5.1.1",
        "bcryptjs": "^3.0.2",
        "body-parser": "^2.2.0",
        "camelcase-keys": "^7.0.2",
        "compression": "^1.8.0",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "csurf": "^1.11.0",
        "date-fns": "^4.1.0",
        "date-fns-tz": "^3.2.0",
        "dayjs": "^1.11.13",
        "dotenv": "^16.5.0",
        "express": "^5.1.0",
        "express-fileupload": "^1.5.1",
        "express-session": "^1.18.1",
        "express-validation": "^4.1.1",
        "helmet": "^8.1.0",
        "hpp": "^0.2.3",
        "http-status": "^2.1.0",
        "i18n": "^0.15.1",
        "jimp": "^1.6.0",
        "joi": "^17.13.3",
        "jsonwebtoken": "^9.0.2",
        "jsqr": "^1.4.0",
        "keycloak-connect": "^26.1.1",
        "lodash": "^4.17.21",
        "mongoose": "^8.14.1",
        "mongoose-aggregate-paginate-v2": "^1.1.4",
        "mongoose-paginate-v2": "^1.9.0",
        "multer": "1.4.5-lts.2",
        "node-schedule": "^2.1.1",
        "onesignal-node": "^3.4.0",
        "pg": "^8.16.0",
        "pino": "^9.6.0",
        "pino-http": "^10.4.0",
        "qs": "^6.14.0",
        "sequelize": "^6.37.7",
        "tesseract.js": "^6.0.1",
        "uuid": "^11.1.0",
        "xlsx": "^0.18.5",
        "yup": "^1.6.1"
    },
    "devDependencies": {
        "@babel/core": "^7.27.4",
        "@babel/preset-env": "^7.27.2",
        "@eslint/js": "^9.0.0",
        "babel-jest": "^30.0.0-beta.3",
        "cross-env": "^7.0.3",
        "eslint": "^9.0.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-prettier": "^5.4.0",
        "globals": "^14.0.0",
        "husky": "^9.1.7",
        "jest": "^29.7.0",
        "jest-cli": "^29.7.0",
        "nodemon": "^3.1.10",
        "prettier": "^3.5.3",
        "sequelize-cli": "^6.6.3",
        "sqlite3": "^5.1.7",
        "supertest": "^7.1.0"
    },
    "lint-staged": {
        "*.{js,json,md}": [
            "prettier --write",
            "eslint --fix"
        ]
    }
}
